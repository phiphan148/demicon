<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd"
                   logicalFilePath="db/changelog-1.0.xml">

    <changeSet author="PhiPhan" id="001">
        <createTable tableName="DEMICON_USER">
            <column name="id" type="integer" autoIncrement="true" incrementBy="1">
                <constraints primaryKey="true" primaryKeyName="user_id_pkey"/>
            </column>
            <column name="name" type="varchar(200)">
            </column>
            <column name="gender" type="varchar(22)">
            </column>
            <column name="email" type="varchar(200)">
            </column>
            <column name="country" type="varchar(200)">
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
